<div class="container">
  <h1> Your New Training Plan </h1>
  <%= button_to "Update Plan!", plan_path(params[:id]), method: :patch%>
  <a href="javascript:void(0)" id="show-hide"> Hide Previous Versions</a>
  <div class="plan-summary">
    <ul>
    </ul>
  </div>
  <div class="row">
    <%= render partial:"calendar", locals: {date: @date, workouts: @workouts, weekly_summaries: @weekly_summaries} %>
  </div>
</div>